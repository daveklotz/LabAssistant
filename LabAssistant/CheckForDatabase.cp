#include "lab assistant.h"#include "database.h"#include "mainMachineList.h"#include "error.h"#include <string.h>#include "machineRecord.h"#include <FIles.h>#include "CheckForDatabase.h"Boolean CheckForDatabase(void){	OSErr err;	short vRefNum = 0;	short fRefNum;		// you do want the "=" here, not "=="	// FSOpen returns 0 when files opens without problems	if (err = FSOpen("\pUser Database", vRefNum, &fRefNum))	{		return err;	}	FSClose(fRefNum);	if (err = FSOpen("\pUser Database Index", vRefNum, &fRefNum))	{		return err;	}	FSClose(fRefNum);	return false;}