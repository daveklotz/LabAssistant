#include "lab assistant.h"#include "listRecord.h"#include "Norm_Files.h"#include <string.h>void CreateList(listNode *theList){	listNode *endNode;		theList = (listNode *) NewPtr(sizeof(listNode));		endNode = (listNode *) NewPtr(sizeof(listNode));		theList->next = endNode;	endNode->next = endNode;}void EmployeeToList(employeeNode *employeeHead, listNode *theList){	employeeNode *theEmployee;	listNode	 theNode;		CreateList(theList);		theEmployee = employeeHead->next;		while(theEmployee->next != theEmployee)	{		theNode.studentNumber = theEmployee->studentNumber;		memcpy(theNode.name, theEmployee->name, sizeof(theNode.name));				AddListRecord(theList, &theNode);				theEmployee = theEmployee->next;	}}void DeleteList(listNode *theList){	listNode *nextNode;		while(theList->next != theList)	{		nextNode = theList->next;				DisposePtr((Ptr) theList);				theList = nextNode;	}		DisposePtr((Ptr) theList);}void AddListRecord(listNode *theList, listNode *theNode){	listNode	*newNode;		newNode = (listNode *) NewPtr(sizeof(listNode));		memcpy(newNode, theNode, sizeof(listNode));		newNode->next = theList->next;		theList->next = newNode;}